     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform vec4 furColor1;
uniform vec4 furColor2;
uniform int nWindFactor;
uniform sampler3D FurDirection;
uniform int nNumPasses;
uniform int nFurOffsetScale;
uniform float furDirectionOffset;
uniform vec4 furColorScale;
uniform float furMessiness;
uniform float pass_index;
uniform sampler2D Fur;

varying vec2 vTexCoord;

void main(void)
{
   // Calculate the texture coord displacement amount
   float fPassIndex = pass_index / float( nNumPasses );
   float furOffsetScale = float( nFurOffsetScale );
   float fWindFactor = float( nWindFactor );
   float fWind = furDirectionOffset * fWindFactor;
   vec2  vTexCoord2 = vec2( vTexCoord.x * furOffsetScale, vTexCoord.y * furOffsetScale );
   vec2  vMessyTexCoord;
   vMessyTexCoord.x = texture3D( FurDirection, vec3( vTexCoord2.x, vTexCoord2.y, fWind ) ).x;
   vMessyTexCoord.y = texture3D( FurDirection, vec3( vTexCoord2.x + 0.5, vTexCoord2.y + 0.5, fWind + 0.5 ) ).y;
   vMessyTexCoord -= 0.5; 
   vMessyTexCoord *= 2.0; // place in [-1.0..1.0] range
   vMessyTexCoord *= ( fPassIndex * furMessiness );
   vMessyTexCoord += vTexCoord;
   
   // Get the alpha component for this shaft
   vec4 fAlpha = texture2D( Fur, vMessyTexCoord );    
    
   // Get the hair shaft color, and scale by the color scale
   vec4 fColor = furColorScale * mix( furColor1, furColor2, fPassIndex ) * fAlpha;
                
   // Return the calculated color
   gl_FragColor = fColor;
}]]></RmShaderCode></RmGLShader>
     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform float shell_distance;
uniform float pass_index;
varying vec2  vTexCoord;

void main(void)
{
   vTexCoord    = gl_MultiTexCoord0.xy;
   
   vec4 Position = gl_Vertex;
   Position.xyz += shell_distance * pass_index * gl_Normal; 
   gl_Position = gl_ModelViewProjectionMatrix * Position;
}
]]></RmShaderCode></RmGLShader>
